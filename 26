#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    float a, b;
    cout << "Введите сторону а" << endl;
    cin >> a;
    cout << "Введите сторону b" << endl;
    cin >> b;
    if ( a == 0  b == 0  a <0 || b < 0) {
        cout << "Нет такого треугольника" << endl;
    }
    else {
    unsigned long gipot = pow(a,2) + pow(b,2);
    cout << gipot << endl;
    }
    return 0;
}
