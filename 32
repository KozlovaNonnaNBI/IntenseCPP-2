#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    float a, b;
    cout << "Введите a" << endl;
    cin >> a;
    cout << "Введите b" << endl;
    cin >> b;
    float a1 = abs(a);
    float b1 = abs(b);
    float x = (a1 + b1)/2;
    float y = sqrt(sqrt(sqrt(a1*b1)));
    cout << x << endl;
    cout << y << endl;
    return 0;
}
