#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    float x, y;
    cout << "Введите x" << endl;
    cin >> x;
    cout << "Введите y" << endl;
    cin >> y;
    float z = x+((2+y)/pow(x,2))/(y+(1/sqrt(pow(x,2)+10)));
    float q = 2.8*sin(x)+abs(y);
    cout << z << endl;
    cout << q << endl;
    return 0;
}
