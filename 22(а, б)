#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    float a, b;
    cout << "Введите 1 число" << endl;
    cin >> a;
    cout << "Введите 2 число" << endl;
    cin >> b;
    float sredarimf = (a + b)/2;
    float sredgeometry = sqrt(sqrt(sqrt(a*b)));
    cout << sredarimf << endl;
    cout << sredgeometry << endl;
    return 0;
}
