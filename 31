#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    float a, b;
    cout << "Введите a" << endl;
    cin >> a;
    cout << "Введите b" << endl;
    cin >> b;
    float x = ((2/(pow(a,2)+25))+b)/(sqrt(b))+((a+b)/2);
    float y = (abs(a)+2*sin(b))/5.5*a;
    cout << x << endl;
    cout << y << endl;
    return 0;
}
