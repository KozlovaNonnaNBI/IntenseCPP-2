#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    float a;
    cout << "Введите a" << endl;
    cin >> a;
    if (a < 0) {
        cout << "Под корнем отрицательное число";
    }
    else {
    float x = sin((3.2 + sqrt(1+a))/abs(5*a));
    cout << x << endl;
    }
    return 0;
}
