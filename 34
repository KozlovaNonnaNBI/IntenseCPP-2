#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    float a, b;
    cout << "Введите a" << endl;
    cin >> a;
    cout << "Введите b" << endl;
    cin >> b;
    float x1 = a + b;
    float x2 = a - b;
    float x3 = a * b;
    float x4 = a / b;
    cout << "Сумма = " << x1 << endl;
    cout << "Разность = " << x2 << endl;
    cout << "Произведение = " << x3 << endl;
    cout << "Частное от Деления = " << x4 << endl;
    return 0;
}
